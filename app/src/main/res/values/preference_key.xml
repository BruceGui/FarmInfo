<?xml version="1.0" encoding="utf-8"?>

<!-- 这个文件用来存储偏好设置的键，这样就可以通过它来获取和改变它相应的值
    在 preference.xml 中使用 android:key="@string/<pref_key>"
    在 代码 中使用 findPreference(getText(R.string.<pref_key>));
 -->

<resources>

    <string name="pref_connection_type_key">pref_connection_param_type</string>
    <string name="pref_server_ip_key">pref_server_ip</string>
    <string name="pref_server_port_key">pref_server_port</string>
    <string name="pref_bluetooth_device_address_key">pref_bluetooth_device_address</string>

    <string name="pref_mobile_station_height_key">pref_mobile_station_height</string>
    <string name="pref_base_station_height_key">pref_base_station_height</string>

</resources>
